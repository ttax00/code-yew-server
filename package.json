{
	"name": "rust-yew",
	"description": "Provides some html language features for yew html macro.",
	"author": "TechTheAwesome",
	"publisher": "TechTheAwesome",
	"version": "0.2.1",
	"pricing": "Free",
	"icon": "rustyew.png",
	"packageManager": "^npm@8.19.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/TechTheAwesome/code-yew-server/"
	},
	"keywords": [
		"rust",
		"yew",
		"html",
		"macro",
		"syntax"
	],
	"engines": {
		"vscode": "^1.72.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"activationEvents": [
		"onLanguage:rustyew"
	],
	"main": "./client/out/extension",
	"contributes": {
		"languages": [
			{
				"id": "rust",
				"aliases": [
					"Rust Yew"
				],
				"extensions": [
					".rs"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "html",
				"configuration": "./html.language-configuration.json"
			}
		],
		"grammars": [
			{
				"injectTo": [
					"source.rust"
				],
				"scopeName": "source.rust.yew",
				"path": "./syntaxes/yew.injection.json",
				"embeddedLanguages": {
					"source.rust.yew.html": "html",
					"source.rust.yew.html.embedded-rust": "rust"
				}
			},
			{
				"injectTo": [
					"source.rust"
				],
				"scopeName": "source.rust.leptos",
				"path": "./syntaxes/leptos.injection.json",
				"embeddedLanguages": {
					"source.rust.leptos.html": "html",
					"source.rust.leptos.embedded-rust": "rust"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"clean": "rimraf ./client/out && rimraf ./server/out",
		"compile": "npm run clean && tsc -b .",
		"watch": "npm run clean && tsc -b -w .",
		"lint": "eslint .",
		"postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
		"test": "jest",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/jest": "^29.2.6",
		"@types/node": "^16.11.7",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"@vscode/vsce": "^2.16.0",
		"eslint": "^8.13.0",
		"eslint-plugin-jsonc": "^2.6.0",
		"jest": "^29.3.1",
		"rimraf": "^4.1.1",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4"
	},
	"extensionDependencies": [
		"rust-lang.rust-analyzer"
	]
}